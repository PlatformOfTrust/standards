{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "@context": {
            "type": "string",
            "minLength": 1
        },
        "@type": {
            "type": "string",
            "minLength": 1,
            "enum": [
                "Building"
            ]
        },
        "data": {
            "title": "",
            "description": "",
            "type": "object",
            "properties": {
                "inaugurationMomentYear": {
                    "title": "Inauguration year",
                    "description": "Inauguration year of the building",
                    "type": "string"
                },
                "name": {
                    "title": "Apartment name",
                    "description": "Name of the building",
                    "type": "string"
                },
                "completionMomentYear": {
                    "title": "Completion year",
                    "description": "Completion year of the building",
                    "type": "string"
                },
                "height": {
                    "title": "Height",
                    "description": "Height of the building",
                    "type": "string"
                },
                "areaSquareMeterFloorNet": {
                    "title": "Floor net square meters",
                    "description": "Net square meters of floor space/area",
                    "type": "string"
                },
                "ifcGuid": {
                    "title": "IFC GUID",
                    "description": "IFC standard based Globally Unique Identifier",
                    "type": "string"
                },
                "volume": {
                    "title": "Volume",
                    "description": "Volume of the building",
                    "type": "string"
                },
                "descriptionGeneral": {
                    "title": "Description",
                    "description": "Description",
                    "type": "string"
                },
                "inspectionMomentYear": {
                    "title": "Inspection year",
                    "description": "Inspection year of the building",
                    "type": "string"
                },
                "idOfficialTemporary": {
                    "title": "Temporary official Id",
                    "description": "Temporary identifier given officially to the building in Finland",
                    "type": "string"
                },
                "areaSquareMeterLivingNet": {
                    "title": "Living space net square meters",
                    "description": "Living space net square meters",
                    "type": "string"
                },
                "idOfficialPermanent": {
                    "title": "Permanent official Id",
                    "description": "Permanent identifier given officially to the building in Finland",
                    "type": "string"
                },
                "status": {
                    "title": "Project status",
                    "description": "Life cycle status of the building",
                    "type": "string"
                },
                "cultureHistorySignificance": {
                    "title": "Cultural or historical significance",
                    "description": "Categorization if building is culturally significant",
                    "type": "string"
                }
            }
        },
        "metadata": {
            "title": "Metadata",
            "description": "Metadata container",
            "type": "object",
            "properties": {
                "updatedAt": {
                    "title": "Update time",
                    "description": "Time when the data is updated",
                    "type": "string"
                },
                "updatedBy": {
                    "title": "Updater",
                    "description": "Updater of an identity",
                    "type": "string"
                },
                "createdAt": {
                    "title": "Creation time",
                    "description": "Time when the data is created",
                    "type": "string"
                },
                "createdBy": {
                    "title": "Creator",
                    "description": "Creator of and identity",
                    "type": "string"
                }
            }
        },
        "id": {
            "title": "",
            "description": "",
            "type": "string"
        }
    },
    "required": [
        "id",
        "data",
        "@context",
        "@type"
    ]
}