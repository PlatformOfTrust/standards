{
    "@context": {
        "@version": 1.1,
        "xsd": {
            "@id": "http://www.w3.org/2001/XMLSchema#",
            "@prefix": true
        },
        "dli": {
            "@id": "https://standards.lifeengine.io/ontologies/dli.jsonld#",
            "@prefix": true
        },
        "pot": {
            "@id": "https://standards.oftrust.net/v1/Vocabulary/",
            "@prefix": true
        },
        "description": {
            "@id": "pot:description",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "label": {
            "@id": "pot:label",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "@vocab": "https://standards.oftrust.net/v1/Vocabulary/Person"
    },
    "@id": "https://standards.oftrust.net/v1/Vocabulary/Person",
    "pot:supportedClass": {
        "@id": "dli:Person",
        "@type": "dli:Class",
        "pot:label": {
            "en-us": "Person"
        },
        "pot:supportedAttribute": {
            "data": {
                "@id": "pot:data",
                "@type": "pot:SupportedAttribute",
                "pot:title": "data",
                "pot:description": {
                    "en-us": "data"
                },
                "pot:required": false
            },
            "descriptionAdditionalInformation": {
                "@id": "pot:descriptionAdditionalInformation",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Additional information",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Additional information"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string",
                    "xsd:maxLength": "500"
                }
            },
            "idBirthdate": {
                "@id": "pot:idBirthdate",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Date of birth",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Birthdate of the person"
                },
                "pot:valueType": [
                    "xsd:date"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:date"
                }
            },
            "firstName": {
                "@id": "pot:idNameFirst",
                "@type": "pot:SupportedAttribute",
                "pot:title": "First Name",
                "pot:required": false,
                "pot:description": {
                    "en-us": "First name of the person"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:maxLength": "50",
                    "xsd:base": "xsd:string"
                }
            },
            "lastName": {
                "@id": "pot:idNameLast",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Last name",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Last name of the person"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string",
                    "xsd:maxLength": "50"
                }
            },
            "middleName": {
                "@id": "pot:idNameMiddle",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Middle names",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Middle names of the person"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:maxLength": "50",
                    "xsd:base": "xsd:string"
                }
            },
            "idOfficial": {
                "@id": "pot:idOfficial",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Identity number",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Official identifier of the person in Finland"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string",
                    "xsd:length": "11"
                }
            },
            "lifeCycleStatusCurrent": {
                "@id": "pot:lifeCycleStatusCurrent",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Life cycle status",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Life cycle status"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:maxLength": "50",
                    "xsd:base": "xsd:string"
                }
            }
        }
    }
}