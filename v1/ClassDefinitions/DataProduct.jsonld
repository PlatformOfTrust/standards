{
    "@context": {
        "@version": 1.1,
        "xsd": {
            "@id": "http://www.w3.org/2001/XMLSchema#",
            "@prefix": true
        },
        "dli": {
            "@id": "https://standards.lifeengine.io/ontologies/dli.jsonld#",
            "@prefix": true
        },
        "pot": {
            "@id": "https://standards.oftrust.net/v1/Vocabulary/",
            "@prefix": true
        },
        "description": {
            "@id": "pot:description",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "label": {
            "@id": "pot:label",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "@vocab": "https://standards.oftrust.net/v1/Vocabulary/DataProduct"
    },
    "@id": "https://standards.oftrust.net/v1/Vocabulary/DataProduct",
    "pot:supportedClass": {
        "@id": "dli:DataProduct",
        "@type": "dli:Class",
        "pot:label": {
            "en-us": "Data Product"
        },
        "pot:supportedAttribute": {
            "data": {
                "@id": "dli:data",
                "@type": "pot:SupportedAttribute",
                "pot:title": "data",
                "pot:description": {
                    "en-us": "data"
                },
                "pot:required": false
            },
            "createdAt": {
                "@id": "dli:createdAt",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Creation time",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Time when the data is created"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string"
                }
            },
            "createdBy": {
                "@id": "dli:createdBy",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Creator",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Creator of and identity "
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string"
                }
            },
            "name": {
                "@id": "dli:name",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Name",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Name of the data product"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:maxLength": "50",
                    "xsd:base": "xsd:string"
                }
            },
            "updatedAt": {
                "@id": "dli:updatedAt",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Update time",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Time when the data is updated"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string"
                }
            },
            "updatedBy": {
                "@id": "dli:updatedBy",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Updater",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Updater of an identity "
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string"
                }
            }
        }
    }
}