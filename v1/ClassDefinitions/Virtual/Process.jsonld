{
    "@context": {
        "@version": 1.1,
        "xsd": {
            "@id": "http://www.w3.org/2001/XMLSchema#",
            "@prefix": true
        },
        "dli": {
            "@id": "https://standards.lifeengine.io/ontologies/dli.jsonld#",
            "@prefix": true
        },
        "pot": {
            "@id": "https://standards.oftrust.net/v1/Vocabulary/",
            "@prefix": true
        },
        "description": {
            "@id": "pot:description",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "label": {
            "@id": "pot:label",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "@vocab": "https://standards.oftrust.net/v1/Vocabulary/Virtual/Process"
    },
    "@id": "https://standards.oftrust.net/v1/Vocabulary/Virtual/Process",
    "pot:supportedClass": {
        "@id": "dli:Virtual/Process",
        "@type": "dli:Class",
        "subClassOf": "dli:Virtual",
        "pot:label": {
            "en-us": "Process"
        },
        "pot:supportedAttribute": {
            "data": {
                "@id": "pot:data",
                "@type": "pot:SupportedAttribute",
                "pot:title": "data",
                "pot:description": {
                    "en-us": "data"
                },
                "pot:required": false
            },
            "description": {
                "@id": "pot:description",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Process description",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Description of the process"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string",
                    "xsd:maxLength": "500"
                }
            },
            "idLocal": {
                "@id": "pot:idLocal",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Process Id",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Locally given identifier for the process instance"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string",
                    "xsd:maxLength": "50"
                }
            },
            "idNameLocal": {
                "@id": "pot:idNameLocal",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Process name - local",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Locally given name (source system name) for the process"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string",
                    "xsd:maxLength": "50"
                }
            },
            "lifeCycleStatusCurrent": {
                "@id": "pot:lifeCycleStatusCurrent",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Process status",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Process state"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:maxLength": "50",
                    "xsd:base": "xsd:string"
                }
            },
            "locationName": {
                "@id": "pot:locationName",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Location",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Location where the process is executed"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:maxLength": "50",
                    "xsd:base": "xsd:string"
                }
            },
            "outputValue": {
                "@id": "pot:outputValue",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Output value",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Output value of the process"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:maxLength": "50",
                    "xsd:base": "xsd:string"
                }
            },
            "outputValueUnit": {
                "@id": "pot:outputValueUnit",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Output value unit",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Unit used for the process output value"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:string",
                    "xsd:maxLength": "50"
                }
            },
            "physicalQuantity": {
                "@id": "pot:physicalQuantity",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Quantity",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Quantity of the processes executed or to be executed"
                },
                "pot:valueType": [
                    "xsd:int"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:int"
                }
            },
            "timeDuration": {
                "@id": "pot:timeDuration",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Duration time",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Duration of the process"
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "xsd:restriction": {
                    "xsd:maxLength": "50",
                    "xsd:base": "xsd:string"
                }
            },
            "timeEndTime": {
                "@id": "pot:timeEndTime",
                "@type": "pot:SupportedAttribute",
                "pot:title": "End time",
                "pot:required": false,
                "pot:description": {
                    "en-us": "End time of the process"
                },
                "pot:valueType": [
                    "xsd:dateTime"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:dateTime"
                }
            },
            "timeStartTime": {
                "@id": "pot:timeStartTime",
                "@type": "pot:SupportedAttribute",
                "pot:title": "Start time",
                "pot:required": false,
                "pot:description": {
                    "en-us": "Start time of the process"
                },
                "pot:valueType": [
                    "xsd:dateTime"
                ],
                "xsd:restriction": {
                    "xsd:base": "xsd:dateTime"
                }
            }
        }
    }
}