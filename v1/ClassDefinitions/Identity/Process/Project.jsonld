{
    "@context": {
        "@version": 1.1,
        "xsd": {
            "@id": "http://www.w3.org/2001/XMLSchema#",
            "@prefix": true
        },
        "pot": {
            "@id": "https://standards-ontotest.oftrust.net/v1/Vocabulary/",
            "@prefix": true
        },
        "description": {
            "@id": "rdfs:comment",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "label": {
            "@id": "rdfs:label",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "comment": {
            "@id": "rdfs:comment",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "@vocab": "https://standards-ontotest.oftrust.net/v1/Vocabulary/Identity/Process/Project"
    },
    "pot:supportedClass": {
        "@id": "pot:Identity/Process/Project",
        "@type": "owl:Class",
        "subClassOf": "pot:Process",
        "rdfs:label": {
            "en-us": "Project",
            "fi-fi": "Projekti"
        },
        "rdfs:comment": {
            "en-us": "Project (or program) is any undertaking, carried out individually or collaboratively, and that is carefully planned to achieve a particular aim.  Typically a project is long-lasting, combines resouces and contains many processes"
        },
        "pot:supportedAttribute": {
            "createdAt": {
                "@id": "pot:metadata/createdAt",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:metadata",
                "pot:required": false,
                "pot:readonly": true
            },
            "createdBy": {
                "@id": "pot:metadata/createdBy",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:metadata",
                "pot:required": false,
                "pot:readonly": true
            },
            "data": {
                "@id": "pot:data",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:topDataProperty",
                "pot:required": true,
                "pot:readonly": false
            },
            "descriptionGeneral": {
                "@id": "pot:description/descriptionGeneral",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:description",
                "pot:required": false,
                "pot:readonly": false,
                "pot:valueType": [
                    "xsd:string"
                ]
            },
            "duration": {
                "@id": "pot:time/duration",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:time",
                "pot:required": false,
                "pot:readonly": false,
                "pot:valueType": [
                    "xsd:string"
                ]
            },
            "durationEstimate": {
                "@id": "pot:time/durationEstimate",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:time",
                "rdfs:label": {
                    "fi-fi": "Arvioitu kesto",
                    "en-us": "Estimated duration"
                },
                "pot:required": false,
                "pot:readonly": false,
                "pot:valueType": [
                    "xsd:string"
                ]
            },
            "endDateTime": {
                "@id": "pot:time/endDateTime",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:time",
                "rdfs:label": {
                    "en-us": "End time",
                    "fi-fi": "Lopetusaika"
                },
                "pot:required": false,
                "pot:readonly": false,
                "pot:valueType": [
                    "xsd:dateTime"
                ]
            },
            "executor": {
                "@id": "pot:role/executor",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:role",
                "rdfs:label": {
                    "en-us": "Process executor",
                    "fi-fi": "Prosessin tekijä"
                },
                "rdfs:comment": {
                    "en-us": "Executor of the process (source of the data)"
                },
                "pot:required": false,
                "pot:readonly": false
            },
            "function": {
                "@id": "pot:function",
                "@type": "owl:ObjectProperty"
            },
            "id": {
                "@id": "pot:identifier/id",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:identifier",
                "pot:required": true,
                "pot:readonly": false
            },
            "idLocal": {
                "@id": "pot:identifier/idLocal",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:identifier",
                "pot:required": false,
                "pot:readonly": false,
                "pot:valueType": [
                    "xsd:string"
                ]
            },
            "locationName": {
                "@id": "pot:location/locationName",
                "@type": "owl:ObjectProperty",
                "subPropertyOf": "pot:location"
            },
            "metadata": {
                "@id": "pot:metadata",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:topDataProperty",
                "pot:required": false,
                "pot:readonly": true
            },
            "nameLocal": {
                "@id": "pot:identifier/nameLocal",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:identifier",
                "pot:required": false,
                "pot:readonly": false,
                "pot:valueType": [
                    "xsd:string"
                ]
            },
            "processTypePoT": {
                "@id": "pot:categorization/processTypePoT",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:categorization",
                "rdfs:label": {
                    "en-us": "Process type - PoT type",
                    "fi-fi": "Prosessin tyyppi - PoT luokitus"
                },
                "rdfs:comment": {
                    "en-us": "PoT defined standard process type"
                },
                "pot:required": false,
                "pot:readonly": false
            },
            "processValue": {
                "@id": "pot:other/processValue",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:other",
                "rdfs:label": {
                    "en-us": "Output value",
                    "fi-fi": "Tuotoksen arvo"
                },
                "rdfs:comment": {
                    "en-us": "Output value of the process"
                },
                "pot:required": false,
                "pot:readonly": false,
                "pot:valueType": [
                    "xsd:string"
                ]
            },
            "startDateTime": {
                "@id": "pot:time/startDateTime",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:time",
                "rdfs:label": {
                    "fi-fi": "Aloitusaika",
                    "en-us": "Start time"
                },
                "pot:required": false,
                "pot:readonly": false,
                "pot:valueType": [
                    "xsd:dateTime"
                ]
            },
            "status": {
                "@id": "pot:lifeCycle/status",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:lifeCycle",
                "pot:required": false,
                "pot:readonly": false,
                "pot:valueType": [
                    "xsd:string"
                ]
            },
            "updatedAt": {
                "@id": "pot:metadata/updatedAt",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:metadata",
                "pot:required": false,
                "pot:readonly": true
            },
            "updatedBy": {
                "@id": "pot:metadata/updatedBy",
                "@type": "owl:DatatypeProperty",
                "subPropertyOf": "pot:metadata",
                "pot:required": false,
                "pot:readonly": true
            }
        }
    }
}