    "data": {
        "@id": "pot:data",
        "@type": "owl:DatatypeProperty",
        "subPropertyOf": "pot:topDataProperty",
        "domain": [
            "pot:Identity",
            "pot:Link"
        ]
    },
    "createdBy": {
        "@id": "pot:metadata/createdBy",
