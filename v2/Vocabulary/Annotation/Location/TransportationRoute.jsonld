            "fi-fi": "Ruuhkataso"
        },
        "rdfs:comment": {
            "en-us": "Congestion level",
            "fi-fi": "Ruuhkataso"
        },
        "pot:valueType": [
            "xsd:string"
        ],
        "domain": [
            "pot:Annotation/Location"
        ]
