    "TransportationRoute": {
        "@id": "pot:Annotation/Location/TransportationRoute",
        "@type": "owl:Class",
        "subClassOf": "pot:Location",
        "rdfs:label": {
            "fi-fi": "Kuljetusreitti",
            "en-us": "Transportation Route"
        },
        "rdfs:comment": {
            "en-us": "A transportation route is the regular path that is followed by a movement of people or goods.",
            "fi-fi": "Kuljetusreitti on säännöllinen reitti jolla kuljetetaan tavaroita tai ihmisiä."
        }
    },
    "idLocal": {
        "@id": "pot:identifier/idLocal",
        "@type": "owl:DatatypeProperty",
        "subPropertyOf": "pot:identifier",
        "rdfs:label": {
            "en-us": "Local identifier",
            "fi-fi": "Paikallinen tunniste"
        },
        "rdfs:comment": {
            "en-us": "Locally given identifier",
            "fi-fi": "Paikallinen tunniste"
        },
        "label": [
            {
                "rdfs:label": {
                    "en-us": "Case Id",
                    "fi-fi": "Palvelupyynnön tunniste"
                },
                "domain": [
                    "pot:Identity/Case"
                ]
            },
            {
                "rdfs:label": {
                    "en-us": "Local code",
                    "fi-fi": "Paikallinen koodi"
                },
                "domain": [
                    "pot:Identity/Equipment/Device"
                ]
            },
            {
                "rdfs:label": {
                    "en-us": "Local identifier",
                    "fi-fi": "Paikallinen tunniste"
                },
                "domain": [
                    "pot:Identity/Element/BuildingElement/Storey"
                ]
            },
            {
                "rdfs:label": {
                    "en-us": "Project Id",
                    "fi-fi": "Projektin tunniste"
                },
                "domain": [
                    "pot:Identity/Project"
                ]
            },
            {
                "rdfs:label": {
                    "en-us": "Local identifier",
                    "fi-fi": "Paikallinen tunniste"
                },
                "domain": [
                    "pot:Identity/Space",
                    "pot:Identity/Space/Room"
                ]
            },
            {
                "rdfs:label": {
                    "en-us": "Local code",
                    "fi-fi": "Paikallinen koodi"
                },
                "domain": [
                    "pot:Identity/Element/BuildingElement"
                ]
            },
            {
                "rdfs:label": {
                    "en-us": "Process Id",
                    "fi-fi": "Prosessin tunniste"
                },
                "domain": [
                    "pot:Identity/Process"
                ]
            }
        ],
        "comment": [
            {
                "rdfs:comment": {
                    "en-us": "Local identifier (system identifier)"
                },
                "domain": [
                    "pot:Identity/Case"
                ]
            },
            {
                "rdfs:comment": {
                    "en-us": "Locally given identifier for the process instance"
                },
                "domain": [
                    "pot:Identity/Process"
                ]
            },
            {
                "rdfs:comment": {
                    "en-us": "Locally given identifier for the storey"
                },
                "domain": [
                    "pot:Identity/Element/BuildingElement/Storey"
                ]
            },
            {
                "rdfs:comment": {
                    "en-us": "Local identifier for the project"
                },
                "domain": [
                    "pot:Identity/Project"
                ]
            },
            {
                "rdfs:comment": {
                    "en-us": "Identifier given locally for the building element"
                },
                "domain": [
                    "pot:Identity/Element/BuildingElement"
                ]
            },
            {
                "rdfs:comment": {
                    "en-us": "Identifier given locally for the device"
                },
                "domain": [
                    "pot:Identity/Equipment/Device"
                ]
            },
            {
                "rdfs:comment": {
                    "en-us": "Locally given identifier for the space"
                },
                "domain": [
                    "pot:Identity/Space",
                    "pot:Identity/Space/Room"
                ]
            }
        ],
        "pot:valueType": [
            "xsd:string"
