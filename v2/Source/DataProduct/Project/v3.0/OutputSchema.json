{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$id": "https://standards-ontotest.oftrust.net/v2/Schema/DataProductOutput/Project",
    "type": "object",
    "required": [],
    "properties": {
        "@context": {
            "$id": "#/properties/@context",
            "type": "string",
            "const": "https://standards-ontotest.oftrust.net/v2/Context/DataProductOutput/Project/",
            "title": "JSON-LD context url",
            "description": "JSON-LD context url with terms required to understand data product content."
        },
        "data": {
            "$id": "#/properties/data",
            "type": "object",
            "title": "Data product output",
            "description": "Output of data product delivered to customers.",
            "required": [],
            "properties": {
                "project": {
                    "$id": "#/properties/data/properties/project",
                    "type": "array",
                    "title": "Project",
                    "description": "Project (or program) is any undertaking, carried out individually or collaboratively, and that is carefully planned to achieve a particular aim.  Typically a project is long-lasting, combines resouces and contains many processes.",
                    "items": {
                        "$id": "#/properties/data/properties/project/items",
                        "type": "object",
                        "required": [],
                        "properties": {
                            "idLocal": {
                                "$id": "#/properties/data/properties/project/items/properties/idLocal",
                                "type": "integer",
                                "title": "Local identifier",
                                "description": "Locally given identifier."
                            },
                            "name": {
                                "$id": "#/properties/data/properties/project/items/properties/name",
                                "type": "string",
                                "title": "Name",
                                "description": "Name."
                            },
                            "url": {
                                "$id": "#/properties/data/properties/project/items/properties/url",
                                "type": "string",
                                "title": "URL address",
                                "description": "URL address."
                            },
                            "categorizationLocal": {
                                "$id": "#/properties/data/properties/project/items/properties/categorizationLocal",
                                "type": "string",
                                "title": "Local category",
                                "description": "Categorisation name given locally."
                            },
                            "nameLocal": {
                                "$id": "#/properties/data/properties/project/items/properties/nameLocal",
                                "type": "string",
                                "title": "Local name",
                                "description": "Local name."
                            },
                            "contactStreetAddress": {
                                "$id": "#/properties/data/properties/project/items/properties/contactStreetAddress",
                                "type": "object",
                                "title": "Contact street address",
                                "description": "Contact street address.",
                                "required": [],
                                "properties": {
                                    "streetAddressLine1": {
                                        "$id": "#/properties/data/properties/project/items/properties/contactStreetAddress/properties/streetAddressLine1",
                                        "type": "string",
                                        "title": "Street address line 1",
                                        "description": "Street address line 1."
                                    }
                                }
                            },
                            "buildingPermission": {
                                "$id": "#/properties/data/properties/project/items/properties/buildingPermission",
                                "type": "object",
                                "title": "Building permission",
                                "description": "Building permission",
                                "required": [],
                                "properties": {
                                    "idOfficial": {
                                        "$id": "#/properties/data/properties/project/items/properties/buildingPermission/properties/idOfficial",
                                        "type": "string",
                                        "title": "Official identifier",
                                        "description": "Government (official authority) assigned identifier."
                                    }
                                }
                            },
                            "process": {
                                "$id": "#/properties/data/properties/project/items/properties/processes",
                                "type": "array",
                                "title": "Process",
                                "description": "A process is a single activity or set of activities that interact to produce a result.",
                                "items": {
                                    "$id": "#/properties/data/properties/project/items/properties/processes/items",
                                    "type": "object",
                                    "required": [],
                                    "properties": {
                                        "@type": {
                                            "$id": "#/properties/data/properties/project/items/properties/processes/items/properties/@type",
                                            "type": "string",
                                            "title": "Identity type",
                                            "description": "Type of identity."
                                        },
                                        "idLocal": {
                                            "$id": "#/properties/data/properties/project/items/properties/processes/items/properties/idLocal",
                                            "type": "integer",
                                            "title": "Local identifier",
                                            "description": "Locally given identifier."
                                        },
                                        "nameLocal": {
                                            "$id": "#/properties/data/properties/project/items/properties/processes/items/properties/nameLocal",
                                            "type": "string",
                                            "title": "Local name",
                                            "description": "Local name."
                                        },
                                        "categorizationLocal": {
                                            "$id": "#/properties/data/properties/project/items/properties/processes/items/properties/categorizationLocal",
                                            "type": "string",
                                            "title": "Local category",
                                            "description": "Categorisation name given locally."
                                        },
                                        "updated": {
                                            "$id": "#/properties/data/properties/project/items/properties/processes/items/properties/updated",
                                            "type": "integer",
                                            "title": "Update time",
                                            "description": "Last update time."
                                        },
                                        "completed": {
                                            "$id": "#/properties/data/properties/project/items/properties/processes/items/properties/completed",
                                            "type": "null",
                                            "title": "Completion time",
                                            "description": "Completion time."
                                        },
                                        "url": {
                                            "$id": "#/properties/data/properties/project/items/properties/processes/items/properties/url",
                                            "type": "string",
                                            "title": "URL address",
                                            "description": "URL address."
                                        },
                                        "status": {
                                            "$id": "#/properties/data/properties/project/items/properties/processes/items/properties/status",
                                            "type": "string",
                                            "title": "Life-cycle status",
                                            "description": "Life-cycle status."
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "signature": {
            "$id": "#/properties/signature",
            "type": "object",
            "title": "Signature",
            "description": "Signature.",
            "required": [],
            "properties": {
                "type": {
                    "$id": "#/properties/signature/properties/type",
                    "type": "string",
                    "title": "Type",
                    "description": "Type."
                },
                "created": {
                    "$id": "#/properties/signature/properties/created",
                    "type": "string",
                    "title": "Created",
                    "description": "Creation time."
                },
                "creator": {
                    "$id": "#/properties/signature/properties/creator",
                    "type": "string",
                    "title": "Creator",
                    "description": "Party who has created the file or information."
                },
                "signatureValue": {
                    "$id": "#/properties/signature/properties/signatureValue",
                    "type": "string",
                    "title": "Signature value",
                    "description": "Signature value."
                }
            }
        }
    }
}