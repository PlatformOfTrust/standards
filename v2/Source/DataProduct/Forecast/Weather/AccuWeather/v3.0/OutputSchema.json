{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$id": "https://standards-ontotest.oftrust.net/v2/Schema/DataProductOutput/Forecast/Weather/AccuWeather",
    "type": "object",
    "required": [],
    "properties": {
        "@context": {
            "$id": "#/properties/@context",
            "type": "string",
            "const": "https://standards-ontotest.oftrust.net/v2/Context/DataProductOutput/Forecast/Weather/AccuWeather/",
            "title": "JSON-LD context url",
            "description": "JSON-LD context url with terms required to understand data product content."
        },
        "data": {
            "$id": "#/properties/data",
            "type": "object",
            "title": "Data product output",
            "description": "Output of data product delivered to customers.",
            "required": [],
            "properties": {
                "forecast": {
                    "$id": "#/properties/data/properties/forecast",
                    "type": "array",
                    "title": "Forecast",
                    "description": "Forecast.",
                    "items": {
                        "$id": "#/properties/data/properties/forecast/items",
                        "type": "object",
                        "required": [],
                        "properties": {
                            "targetObject": {
                                "$id": "#/properties/data/properties/forecast/items/properties/targetObject",
                                "type": "object",
                                "title": "Target object",
                                "description": "Target object",
                                "required": [],
                                "properties": {
                                    "type": {
                                        "$id": "#/properties/data/properties/forecast/items/properties/targetObject/properties/type",
                                        "type": "string",
                                        "title": "Identity type",
                                        "description": "Type of identity."
                                    },
                                    "latitude": {
                                        "$id": "#/properties/data/properties/forecast/items/properties/targetObject/properties/latitude",
                                        "type": "number",
                                        "title": "Latitude",
                                        "description": "The angular distance north or south from the equator of a point on the earth's surface, measured on the meridian of the point (WGS 84)."
                                    },
                                    "longitude": {
                                        "$id": "#/properties/data/properties/forecast/items/properties/targetObject/properties/longitude",
                                        "type": "number",
                                        "title": "Longitude",
                                        "description": "Angular distance east or west on the earth's surface, measured by the angle contained between the meridian of a particular place and some prime meridian, as that of Greenwich, England, and expressed either in degrees or by some corresponding difference in time (WGS 84)."
                                    }
                                }
                            },
                            "cloudCoverage": {
                                "$id": "#/properties/data/properties/forecast/items/properties/cloudCoverage",
                                "type": "integer",
                                "title": "Cloud coverage",
                                "description": "Cloud coverage."
                            },
                            "dateTime": {
                                "$id": "#/properties/data/properties/forecast/items/properties/dateTime",
                                "type": "string",
                                "title": "Date time",
                                "description": "Date time."
                            },
                            "humidity": {
                                "$id": "#/properties/data/properties/forecast/items/properties/humidity",
                                "type": "integer",
                                "title": "Humidity",
                                "description": "Humidity."
                            },
                            "rainProbability": {
                                "$id": "#/properties/data/properties/forecast/items/properties/rainProbability",
                                "type": "number",
                                "title": "Rain probability",
                                "description": "Rain probability."
                            },
                            "rainVolume": {
                                "$id": "#/properties/data/properties/forecast/items/properties/rainVolume",
                                "type": "integer",
                                "title": "Rain volume",
                                "description": "Rain volume."
                            },
                            "snowProbability": {
                                "$id": "#/properties/data/properties/forecast/items/properties/snowProbability",
                                "type": "integer",
                                "title": "Snow probability",
                                "description": "Snow probability."
                            },
                            "snowVolume": {
                                "$id": "#/properties/data/properties/forecast/items/properties/snowVolume",
                                "type": "integer",
                                "title": "Snow volume",
                                "description": "Snow volume."
                            },
                            "temperature": {
                                "$id": "#/properties/data/properties/forecast/items/properties/temperature",
                                "type": "number",
                                "title": "Temperature",
                                "description": "Temperature of the object."
                            },
                            "temperatureFeel": {
                                "$id": "#/properties/data/properties/forecast/items/properties/temperatureFeel",
                                "type": "number",
                                "title": "Temperature feel",
                                "description": "Temperature feel."
                            },
                            "temperatureMax": {
                                "$id": "#/properties/data/properties/forecast/items/properties/temperatureMax",
                                "type": "number",
                                "title": "Temperature max",
                                "description": "Temperature maximum."
                            },
                            "temperatureFeelMax": {
                                "$id": "#/properties/data/properties/forecast/items/properties/temperatureFeelMax",
                                "type": "number",
                                "title": "Temperature feel max",
                                "description": "Temperature feel maximum."
                            },
                            "temperatureMin": {
                                "$id": "#/properties/data/properties/forecast/items/properties/temperatureMin",
                                "type": "number",
                                "title": "Temperature min",
                                "description": "Temperature minimum."
                            },
                            "temperatureFeelMin": {
                                "$id": "#/properties/data/properties/forecast/items/properties/temperatureFeelMin",
                                "type": "number",
                                "title": "Temperature feel min.",
                                "description": "Temperature feel minimum."
                            },
                            "windDirection": {
                                "$id": "#/properties/data/properties/forecast/items/properties/windDirection",
                                "type": "number",
                                "title": "Wind direction",
                                "description": "Direction of the wind."
                            },
                            "windSpeed": {
                                "$id": "#/properties/data/properties/forecast/items/properties/windSpeed",
                                "type": "number",
                                "title": "Wind speed",
                                "description": "Wind speed."
                            }
                        }
                    }
                }
            }
        },
        "signature": {
            "$id": "#/properties/signature",
            "type": "object",
            "title": "Signature",
            "description": "Signature.",
            "required": [],
            "properties": {
                "type": {
                    "$id": "#/properties/signature/properties/type",
                    "type": "string",
                    "title": "Type",
                    "description": "Type."
                },
                "created": {
                    "$id": "#/properties/signature/properties/created",
                    "type": "string",
                    "title": "Created",
                    "description": "Creation time."
                },
                "creator": {
                    "$id": "#/properties/signature/properties/creator",
                    "type": "string",
                    "title": "Creator",
                    "description": "Party who has created the file or information."
                },
                "signatureValue": {
                    "$id": "#/properties/signature/properties/signatureValue",
                    "type": "string",
                    "title": "Signature value",
                    "description": "Signature value."
                }
            }
        }
    }
}