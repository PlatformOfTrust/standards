{
   "$schema": "http://json-schema.org/draft-06/schema#",
   "$id": "https://standards-ontotest.oftrust.net/v2/Schema/DataProductParameters/Forecast/AccuWeather",
   "type": "object",
   "required": [],
   "properties": {
       "@context": {
           "$id": "#/properties/@context",
           "type": "string",
           "const": "https://standards-ontotest.oftrust.net/v2/Context/DataProductParameters/Forecast/AccuWeather/",
           "title": "JSON-LD context url",
           "description": "JSON-LD context url with terms required to understand data product content."
       },
       "timestamp": {
           "$id": "#/properties/timestamp",
           "type": "string",
           "title": "Time stamp",
           "description": "Time stamp."
       },
       "productCode": {
           "$id": "#/properties/productCode",
           "type": "string",
           "title": "Product code",
           "description": "Code of the product."
       },
       "parameters": {
           "$id": "#/properties/parameters",
           "type": "object",
           "title": "Data product parameters",
           "description": "Data product parameters.",
           "required": [],
           "properties": {
               "period": {
                   "$id": "#/properties/parameters/properties/period",
                   "type": "string",
                   "title": "Period",
                   "description": "A length or portion of time."
               },
               "targetObject": {
                   "$id": "#/properties/parameters/properties/targetObject",
                   "type": "array",
                   "title": "Target object",
                   "description": "Target object.",
                   "items": {
                       "$id": "#/properties/parameters/properties/targetObject/items",
                       "type": "object",
                       "required": [],
                       "properties": {
                           "@type": {
                               "$id": "#/properties/parameters/properties/targetObject/items/properties/type",
                               "type": "string",
                               "title": "Identity type",
                               "description": "Identity type."
                           },
                           "latitude": {
                               "$id": "#/properties/parameters/properties/targetObject/items/properties/latitude",
                               "type": "number",
                               "title": "Latitude",
                               "description": "The angular distance north or south from the equator of a point on the earth's surface, measured on the meridian of the point (WGS 84)."
                           },
                           "longitude": {
                               "$id": "#/properties/parameters/properties/targetObject/items/properties/longitude",
                               "type": "number",
                               "title": "Longitude",
                               "description": "Angular distance east or west on the earth's surface, measured by the angle contained between the meridian of a particular place and some prime meridian, as that of Greenwich, England, and expressed either in degrees or by some corresponding difference in time (WGS 84)."
                           }
                       }
                   }
               }
           }
       }
   }
}