                                "title": "Id source",
                                "description": "Id in the source system."
                            },
                            "documents": {
                                "$id": "#/properties/data/properties/document/items/properties/documents",
                                "type": "array",
                                "title": "Documents",
                                "description": "Documents.",
                                "items": {
                                    "$id": "#/properties/data/properties/document/items/properties/documents/items",
                                    "type": "object",
                                    "required": [],
                                    "properties": {
                                        "@type": {
                                            "$id": "#/properties/data/properties/document/items/properties/documents/items/properties/@type",
                                            "type": "string",
                                            "title": "Identity type",
                                            "description": "Type of identity."
                                        },
                                        "name": {
                                            "$id": "#/properties/data/properties/document/items/properties/documents/items/properties/name",
                                            "type": "string",
                                            "title": "Name",
                                            "description": "Name."
                                        }
                                    }
                                }
                            },
                            "digitalSignatures": {
                                "$id": "#/properties/data/properties/document/items/properties/digitalSignatures",
                                "type": "array",
                                "title": "Digital signatures",
                                "description": "Digital signatures.",
                                "items": {
                                    "$id": "#/properties/data/properties/document/items/properties/digitalSignatures/items",
                                    "type": "object",
                                    "required": [],
                                    "properties": {
                                        "@type": {
                                            "$id": "#/properties/data/properties/document/items/properties/digitalSignatures/items/properties/@type",
                                            "type": "string",
                                            "title": "Identity type",
                                            "description": "Type of identity."
                                        },
                                        "executor": {
                                            "$id": "#/properties/data/properties/document/items/properties/digitalSignatures/items/properties/executor",
                                            "type": "string",
                                            "title": "Process executor",
                                            "description": "Executor of the process (source of the data)."
                                        },
                                        "timestamp": {
                                            "$id": "#/properties/data/properties/document/items/properties/digitalSignatures/items/properties/timestamp",
                                            "type": "string",
                                            "title": "System time stamp",
                                            "description": "System time stamp deriving typically from computer system.  Time when record (file) was created."
                                        }
                                    }
                                }
                            },
                            "creator": {
                                "$id": "#/properties/data/properties/document/items/properties/creator",
                                "type": "object",
                                "title": "Creator",
                                "description": "Party who has created the file or information.",
                                "required": [],
                                "properties": {
                                    "@type": {
                                        "$id": "#/properties/data/properties/document/items/properties/creator/properties/@type",
                                        "type": "string",
                                        "title": "Identity type",
                                        "description": "Type of identity."
