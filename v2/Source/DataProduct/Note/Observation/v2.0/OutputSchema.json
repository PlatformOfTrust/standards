{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$id": "https://standards-ontotest.oftrust.net/v2/Schema/DataProductOutput/Note/Observation",
    "type": "object",
    "required": [],
    "properties": {
        "@context": {
            "$id": "#/properties/@context",
            "type": "string",
            "const": "https://standards-ontotest.oftrust.net/v2/Context/DataProductOutput/Note/Observation/",
            "title": "JSON-LD context url",
            "description": "JSON-LD context url with terms required to understand data product content."
        },
        "data": {
            "$id": "#/properties/data",
            "type": "object",
            "title": "Data product output",
            "description": "Output of data product delivered to customers.",
            "required": [],
            "properties": {
                "observation": {
                    "$id": "#/properties/data/properties/observation",
                    "type": "array",
                    "title": "Observation",
                    "description": "Observation",
                    "items": {
                        "$id": "#/properties/data/properties/observation/items",
                        "type": "object",
                        "required": [],
                        "properties": {
                            "@type": {
                                "$id": "#/properties/data/properties/observation/items/properties/@type",
                                "type": "string",
                                "title": "Identity type",
                                "description": "Type of identity."
                            },
                            "idLocal": {
                                "$id": "#/properties/data/properties/observation/items/properties/idLocal",
                                "type": "string",
                                "title": "Local identifier",
                                "description": "Locally given identifier."
                            },
                            "name": {
                                "$id": "#/properties/data/properties/observation/items/properties/name",
                                "type": "string",
                                "title": "Name",
                                "description": "Name."
                            },
                            "source": {
                                "$id": "#/properties/data/properties/observation/items/properties/source",
                                "type": "string",
                                "title": "Source",
                                "description": "Source of the information."
                            },
                            "categorizationSource": {
                                "$id": "#/properties/data/properties/observation/items/properties/categorizationSource",
                                "type": "string",
                                "title": "Source Category",
                                "description": "Source category describes the type of source where information is derived."
                            },
                            "categorizationLocal": {
                                "$id": "#/properties/data/properties/observation/items/properties/categorizationLocal",
                                "type": "string",
                                "title": "Local category",
                                "description": "Categorisation name given locally."
                            },
                            "categorizationLocal2": {
                                "$id": "#/properties/data/properties/observation/items/properties/categorizationLocal2",
                                "type": "array",
                                "title": "Second local category",
                                "description": "Additional categorization given locally (source system).",
                                "items": {
                                    "$id": "#/properties/data/properties/observation/items/properties/categorizationLocal2/items",
                                    "type": "string"
                                }
                            },
                            "categorizationContext": {
                                "$id": "#/properties/data/properties/observation/items/properties/categorizationContext",
                                "type": "string",
                                "title": "Categorization context",
                                "description": "Categorization context."
                            },
                            "descriptionGeneral": {
                                "$id": "#/properties/data/properties/observation/items/properties/descriptionGeneral",
                                "type": "string",
                                "title": "Description",
                                "description": "Description."
                            },
                            "imageUrl": {
                                "$id": "#/properties/data/properties/observation/items/properties/imageUrl",
                                "type": "string",
                                "title": "Product image",
                                "description": "Link to product image."
                            }
                        }
                    }
                }
            }
        },
        "signature": {
            "$id": "#/properties/signature",
            "type": "object",
            "title": "Signature",
            "description": "Signature.",
            "required": [],
            "properties": {
                "type": {
                    "$id": "#/properties/signature/properties/type",
                    "type": "string",
                    "title": "Type",
                    "description": "Type."
                },
                "created": {
                    "$id": "#/properties/signature/properties/created",
                    "type": "string",
                    "title": "Created",
                    "description": "Creation time."
                },
                "creator": {
                    "$id": "#/properties/signature/properties/creator",
                    "type": "string",
                    "title": "Creator",
                    "description": "Party who has created the file or information."
                },
                "signatureValue": {
                    "$id": "#/properties/signature/properties/signatureValue",
                    "type": "string",
                    "title": "Signature value",
                    "description": "Signature value."
                }
            }
        }
    }
}