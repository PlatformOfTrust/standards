                                        "@type": {
                                            "$id": "#/properties/data/properties/note/items/properties/status/items/properties/@type",
                                            "type": "string",
                                            "title": "Identity type",
                                            "description": "Type of identity."
                                        },
                                        "name": {
                                            "$id": "#/properties/data/properties/note/items/properties/status/items/properties/name",
                                            "type": "string",
                                            "title": "Name",
                                            "description": "Name."
