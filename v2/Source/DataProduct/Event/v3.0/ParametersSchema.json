{
   "$schema": "http://json-schema.org/draft-06/schema#",
   "$id": "https://standards-ontotest.oftrust.net/v2/Schema/DataProductParameters/Event",
   "type": "object",
   "required": [],
   "properties": {
       "@context": {
           "$id": "#/properties/@context",
           "type": "string",
           "const": "https://standards-ontotest.oftrust.net/v2/Context/DataProductParameters/Event/",
           "title": "JSON-LD context url",
           "description": "JSON-LD context url with terms required to understand data product content."
       },
       "productCode": {
           "$id": "#/properties/productCode",
           "type": "string",
           "title": "Product code",
           "description": "Code of the product."
       },
       "timestamp": {
           "$id": "#/properties/timestamp",
           "type": "string",
           "title": "Time stamp",
           "description": "Time stamp."
       },
       "parameters": {
           "$id": "#/properties/parameters",
           "type": "object",
           "title": "Data product parameters",
           "description": "Data product parameters.",
           "required": [],
           "properties": {
               "period": {
                   "$id": "#/properties/parameters/properties/period",
                   "type": "string",
                   "title": "Period",
                   "description": "A length or portion of time."
               },
               "targetObject": {
                   "$id": "#/properties/parameters/properties/targetObject",
                   "type": "array",
                   "title": "Target object",
                   "description": "Target object.",
                   "items": {
                       "$id": "#/properties/parameters/properties/targetObject/items",
                       "type": "object",
                       "required": [],
                       "properties": {
                           "@type": {
                               "$id": "#/properties/parameters/properties/targetObject/items/properties/@type",
                               "type": "string",
                               "title": "Identity type",
                               "description": "Type of identity."
                           },
                           "idLocal": {
                               "$id": "#/properties/parameters/properties/targetObject/items/properties/idLocal",
                               "type": "string",
                               "title": "Local identifier",
                               "description": "Locally given identifier."
                           }
                       }
                   }
               }
           }
       }
   }
}