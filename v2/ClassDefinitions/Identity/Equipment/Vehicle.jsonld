                },
                "rdfs:comment": {
                    "fi-fi": "Merkki yleisesti bussin, junan tai muun kuljetusvälineen edessä jossa tieto suunnasta, määränpäästä, reittinumerosta tai muusta matkaan liittyvästä tiedosta.",
                    "en-us": "The sign on the front of a bus or train (any vehicle) indicating its direction, destination, route number, or other pertinent information."
                },
                "pot:valueType": [
                    "xsd:string"
                ],
                "pot:required": false
