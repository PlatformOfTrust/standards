{
  "@context": {
    "@version": 1.1,
    "@vocab": "https://platformoftrust.github.io/standards/vocabularies/user.jsonld#",
    "pot": {
      "@id": "https://platformoftrust.github.io/standards/ontologies/pot.jsonld#",
      "@prefix": true
    },
    "dli": {
      "@id": "https://digitalliving.github.io/standards/ontologies/dli.jsonld#",
      "@prefix": true
    },
    "data": "dli:data",
    "name": "pot:name",
    "email": "dli:email",
    "password": "dli:password",
    "firstName": {
      "@id": "dli:firstName",
      "@nest": "data"
    },
    "lastName": {
      "@id": "dli:lastName",
      "@nest": "data"
    }
  }
}
