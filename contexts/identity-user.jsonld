{
  "@context": {
    "@version": 1.1,
    "pot": "https://platformoftrust.github.io/standards/ontologies/pot.jsonld#",
    "name": "pot:name",
    "username": "pot:username",
    "password": "pot:password",
    "data": "@nest",
    "first_name": {
      "@id": "pot:first_name",
      "@nest": "data"
    },
    "last_name": {
      "@id": "pot:last_name",
      "@nest": "data"
    },
    "created_at": "pot:created_at",
    "updated_at": "pot:updated_at",
    "created_by": "pot:created_by",
    "updated_by": "pot:updated_by"
  }
}
