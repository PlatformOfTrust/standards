{
  "@context": {
    "@version": 1.1,
    "@vocab": "https://standards.oftrust.net/vocabularies/data_product.jsonld#",
    "sec": {
      "@id": "https://w3id.org/security/v2",
      "@prefix": true
    },
    "pot": {
      "@id": "https://standards.oftrust.net/ontologies/pot.jsonld#",
      "@prefix": true
    },
    "name": "pot:name",
    "description": "pot:description",
    "productCode": "pot:productCode",
    "dataContext": "pot:dataContext",
    "parameterContext": "pot:parameterContext",
    "translatorUrl": "pot:translatorUrl",
    "organizationPublicKeys": {
      "@id": "pot:organizationPublicKeys",
      "@container": "@set"
    },
    "type": {
      "@id": "pot:organizationPublicKeyType",
      "@type": "sec:RsaSignature2018",
      "@nest": "organizationPublicKeys"
    },
    "url": {
      "@id": "pot:organizationPublicKeyUrl",
      "@type": "@id",
      "@nest": "organizationPublicKeys"
    },
    "imageUrl": "pot:imageUrl"
  }
}
