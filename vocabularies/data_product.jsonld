{
  "@context": {
    "@version": 1.1,
    "rdf": {
      "@id": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
      "@prefix": true
    },
    "rdfs": {
      "@id": "http://www.w3.org/2000/01/rdf-schema#",
      "@prefix": true
    },
    "dli": {
      "@id": "https://digitalliving.github.io/standards/ontologies/dli.jsonld#",
      "@prefix": true
    },
    "pot": {
      "@id": "https://platformoftrust.github.io/standards/ontologies/pot.jsonld#",
      "@prefix": true
    },
    "vocab": "https://platformoftrust.github.io/standards/vocabularies/data_product.jsonld#"
  },
  "@id": "https://platformoftrust.github.io/standards/vocabularies/data_product.jsonld",
  "@type": "pot:Vocabulary",
  "pot:supportedClass": [
    {
      "@id": "dli:DataProduct",
      "@type": "dli:DataProduct",
      "dli:title": "Data product",
      "dli:description": null,
      "pot:supportedAttribute": [
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:name",
          "dli:title": "Name",
          "dli:description": "The data product name",
          "dli:required": true
        },
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:description",
          "dli:title": "Description",
          "dli:description": "The description of the data product",
          "dli:required": false
        },
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:productCode",
          "dli:title": "Product code",
          "dli:description": "The product code of the data product",
          "dli:required": true
        },
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:dataContext",
          "dli:title": "Data context",
          "dli:description": "The URL to the context describing the data from the translator",
          "dli:required": true
        },
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:parameterContext",
          "dli:title": "Parameter context",
          "dli:description": "The URL to the context describing the parameters sent to the translator",
          "dli:required": true
        },
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:translatorUrl",
          "dli:title": "Translator URL",
          "dli:description": "The URL to the translator",
          "dli:required": true
        },
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:organizationSignatureType",
          "dli:title": "Organization signature type",
          "dli:description": "The type of the signature used for signing",
          "dli:required": true
        },
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:organizationPublicKeyUrl",
          "dli:title": "Organization public key URL",
          "dli:description": "The URL to the organization's public key used for signing",
          "dli:required": true
        },
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:imageUrl",
          "dli:title": "Image URL",
          "dli:description": "The URL to the image for the data product",
          "dli:required": false
        },
        {
          "@type": "pot:SupportedAttribute",
          "dli:attribute": "pot:sharedSecret",
          "dli:title": "Shared secret",
          "dli:description": "The shared secret known only to the translator and data broker",
          "dli:required": true
        }
      ]
    }
  ]
